module bug.

Unit ◂ ★ = ∀ X : ★ . X ➔ X.
unit ◂ Unit = Λ X . λ x . x.

bug ◂ ∀ P : Unit ➔ ★ . 
      (Π u : Unit. P u) ➔ 
      Π q : Unit .
      P q =  Λ P . λ f . λ q .      
    [ unit' ◂ Unit = q      ]-  
    [ unit'' ◂ Unit = unit' ]-
     (f unit'').


nobug ◂ ∀ P : Unit ➔ ★ . 
      (Π u : Unit. P u) ➔ 
      Π q : Unit .
      P q =  Λ P . λ f . λ q .      
    [ unit' ◂ Unit = q ]-  
%   let unit'' ◂ Unit = unit' in 
    [ prf ◂ P unit' = f unit' ]- prf .
